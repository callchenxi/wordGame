/* components/w-my-team/w-my-team.wxss */
.team-list{
  display: flex;
  width: 720rpx;
  margin: 0 auto;
}
.team-box .team {
  position: relative;
  background-color: goldenrod;
  text-align: center;
  padding-bottom: 4rpx;
}
.team-box .team-pic {
  width: 140rpx;
  height: 200rpx;
  padding: 0 2rpx;
}
.pic-border {
  position: absolute;
  left: 0; 
  z-index: 99;  
}